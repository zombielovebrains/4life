{
  "name": "tf",
  "version": "1.0.0",
  "description": "Портфолио сайт о трансфер факторе",
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "browser-sync": "^2.26.7",
    "editorconfig-cli": "^0.3.0",
    "gulp": "^4.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "stylelint": "^13.2.1",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
